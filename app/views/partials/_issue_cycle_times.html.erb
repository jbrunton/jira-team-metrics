<div class="<%= 'active' if expand %> title">
  <i class="dropdown icon"></i>
  Cycle Time
</div>
<div class="<%= 'active' if expand %> content">
  <table class="issue-details ui definition table">
    <tr>
      <td>Started</td>
      <td><%= pretty_print_time(issue.started_time) %></td>
    </tr>
    <tr>
      <td>Completed</td>
      <td><%= pretty_print_time(issue.completed_time) %></td>
    </tr>
    <tr>
      <td>Days</td>
      <td><%= pretty_print_number(issue.cycle_time) %></td>
    </tr>
  </table>
</div>

