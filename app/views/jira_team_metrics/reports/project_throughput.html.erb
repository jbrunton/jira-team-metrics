<% breadcrumb :project_throughput_report, @project, @team %>
<% content_for :title, "#{@team} Throughput - #{@project.key} - #{@board.name}" %>

<% chart_id = generate_id %>

<%= render partial: 'partials/chart', locals: {
    chart_id: chart_id,
    chart_type: 'ComboChart',
    url: "#{board_api_path(@board)}/throughput/#{@team}.json?query=project='#{@project.key}'".html_safe
} %>

<script>
    function buildComponentUrl(path) {
        return path;
    }
</script>