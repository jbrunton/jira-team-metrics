<% content_for :nav_content do %>
    <a href="<%= domain_path(@domain) %>" class="breadcrumb"><%= @domain.config.name %></a>
    <a href="<%= board_path(@domain, @board) %>" class="breadcrumb"><%= @board.name %></a>
    <a href="#!" class="breadcrumb">Issues</a>
<% end %>

<h3>Issues By Type</h3>

<%= render partial: 'partials/cycle_time_form' %>

<div class="row">
  <div class="col s6 card">
    <%= render partial: 'charts/issue_count_summary' %>
  </div>
  <div class="col s6 card">
    <%= render partial: 'charts/issue_effort_summary' %>
  </div>
</div>

<div class="row">
  <div class="col s12 card">
    <%= render partial: 'charts/issue_count_by_month' %>
  </div>
</div>

<div class="row">
  <div class="col s12 card">
    <%= render partial: 'charts/issue_effort_by_month' %>
  </div>
</div>

<div class="row">
  <div class="col s12 card">
    <%= render partial: 'charts/created_issues_by_month' %>
  </div>
</div>