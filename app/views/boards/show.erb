<% content_for :nav_content do %>
    <a href="<%= domains_path %>" class="breadcrumb">Domains</a>
    <a href="<%= domain_path(@domain) %>" class="breadcrumb"><%= @domain['name'] %></a>
    <a href="#!" class="breadcrumb"><%= @board.name %></a>
<% end %>

<h3><%= @board.name %></h3>

<div class="row">
  <div class="col s12">
    <ul class="collection with-header">
      <li class="collection-header"><h4>Reports</h4></li>
      <li class="collection-item"><div><a href="<%= reports_path(@domain, @board) %>/control_chart">Control Chart</a></div></li>
      <li class="collection-item"><div><a href="<%= reports_path(@domain, @board) %>/issues">Issues</div></li>
      <li class="collection-item"><div><a href="<%= reports_path(@domain, @board) %>/issues_by_type">Issues by Type</div></li>
      <li class="collection-item"><div><a href="<%= reports_path(@domain, @board) %>/cycle_times_by_type">Cycle Times by Type</div></li>
    </ul>
  </div>
</div>

</body>
