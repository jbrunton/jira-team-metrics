<h3>Domains</h3>

<table>
  <thead>
    <tr>
      <td>Name</td>
      <td>Url</td>
      <td></td>
    </tr>
  </thead>
  <% @domains.each do |domain| %>
      <tr>
        <td><%= domain['name'] %></td>
        <td><%= domain['url'] %></td>
        <td>
          <a href="<%= domain_path(domain) %>" class="waves-effect waves-light btn">
            View Details
          </a>
          <a href="<%= domain_path(domain) %>/sync" class="waves-effect waves-light btn">Process</a>
        </td>
      </tr>
  <% end %>
</table>
