<head>
  <!--Import Google Icon Font-->
  <link href="http://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">

  <!--Import materialize.css-->
  <link type="text/css" rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.98.2/css/materialize.min.css" media="screen,projection"/>

  <!-- Compiled and minified JavaScript -->
  <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.98.2/js/materialize.min.js"></script>

  <!--Let browser know website is optimized for mobile-->
  <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
</head>
<body>

  <nav>
    <div class="nav-wrapper container">
      <a href="#!" class="brand-logo"><%= @board.name %></a>

      <ul id="nav-mobile" class="right hide-on-med-and-down">
        <li><a href="<%= board_summary_url(@board) %>">Summary</a></li>
        <li class="active"><a href="#!">All Issues</a></li>
      </ul>
    </div>
  </nav>

  <div class="container">
    <%= print_table(@board.issues_table) %>
  </div>

</body>
